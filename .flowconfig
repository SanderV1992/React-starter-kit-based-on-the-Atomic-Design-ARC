[ignore]
.*/dist
.*/build
.*/node_modules
.*/public
.*/.git
.*/.cache
.*/.idea
.*/.vscode
.*/coverage
.*/config
.*/src/__tests__
.**/*.test.js
.**/*.spec.js
.*/storybook-static

[include]

[libs]

[lints]

[options]
all=true
module.system.node.resolve_dirname=node_modules
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

[strict]
